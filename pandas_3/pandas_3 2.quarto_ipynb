    "#| code-fold: false\n",
    "def ratio_to_peak(series):\n",
    "    return series.iloc[-1] / max(series)\n",
    "\n",
    "#Using .groupby() to apply the function\n",
    "rtp_table = f_babynames.groupby(\"Name\")[[\"Year\", \"Count\"]].agg(ratio_to_peak)\n",
    "rtp_table.head()"
   ]
